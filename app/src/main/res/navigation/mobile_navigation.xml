<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_user_number">

    <fragment
        android:id="@+id/nav_user_number"
        android:name="com.make.deve.pruebacoink.ui.register.fragments.UserPhoneFragment"
        tools:layout="@layout/fragment_user_number">
    </fragment>

    <fragment
        android:id="@+id/nav_user_info"
        android:name="com.make.deve.pruebacoink.ui.register.fragments.UserInfoFragment"
        tools:layout="@layout/fragment_user_info">
    </fragment>

    <fragment
        android:id="@+id/nav_user_finish"
        android:name="com.make.deve.pruebacoink.ui.register.fragments.UserFinishFragment"
        tools:layout="@layout/fragment_user_finish">
    </fragment>

    <dialog
        android:id="@+id/nav_success"
        android:name="com.make.deve.pruebacoink.ui.register.fragments.SuccessDialogFragment"
        tools:layout="@layout/fragment_success" />

</navigation>